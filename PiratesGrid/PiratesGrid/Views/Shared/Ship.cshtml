
@model PirateApp.Domain.Ship


<div class="row form-group">
    <div class="col-md-4 col-md-offset-2">
        <label asp-for="Name">Nazwa Statku</label>
    </div>
    <div class="col-md-2">
        <input class="form-control" asp-for="Name" />
    </div>
</div>

<div class="col-md-4 col-md-offset-2">
    <label asp-for="Power">Moc Statku</label>
</div>
<div class="col-md-2">
    <input class="form-control" asp-for="Power" />
</div>


<div class="col-md-4 col-md-offset-2">
    <label asp-for="HowManyPeopleCanTake">Jak wiele ludzi pomieści</label>
</div>
<div class="col-md-4">
    <input class="form-control" asp-for="HowManyPeopleCanTake" />
</div>




